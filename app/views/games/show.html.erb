<%= @game.name%>

<table class="player">
  <thead>
    <tr>
      <th>Players</th>
      <%@players.each do |p|%>
      <th><%= p.username%></th>
      <% end%>  
    </tr>
 </thead>   
    <tr>
	  <th>Stock</th>
      <%@players.each do |p|%>
	    <td><%= p.scores.find_by_game_id(@game.id).stocks%></td>
	  <%end%>
    </tr>
</table>    